pipeline {
                agent any
                tools{
                           jdk  "JAVA_HOME"
	      maven "maven"
        dockerTool "docker"
                           }



              stages {
        
                             stage("cleanup workspace"){

                                         steps{
                                                     cleanWs()
                                                    }
                                       }
                                        stage("Build Application"){
            steps {
                sh "mvn clean package"
            }

        }

        stage("Test Application"){
            steps {
                sh "mvn test"
            }

        }
                               

                            
              

                                 
              }      
                            }
            
        
    
          
